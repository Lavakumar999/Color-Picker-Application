<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lavakumar.colorchoose">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".content" />
        <activity android:name=".Colorwhite" />
        <activity android:name=".ColorRed" />
        <activity android:name=".ColorYellow" />
        <activity android:name=".ColorBlue" />
        <activity android:name=".ColorBlack" />
        <activity android:name=".colorBrown" />
        <activity android:name=".ColorPink" />
        <activity android:name=".ColorViolet" />
        <activity android:name=".ColorGreen" />
        <activity android:name=".ColorOrange"></activity>
    </application>

</manifest>